    </AR-PACKAGE>
    <AR-PACKAGE>
        <SHORT-NAME>CounterConfigures</SHORT-NAME>
        <ELEMENTS>
            <COUNTER>
                <SHORT-NAME>CounterA</SHORT-NAME>
                <MINCYCLE>10</MINCYCLE>
                <MAXALLOWEDVALUE>39</MAXALLOWEDVALUE>
                <TICKSPERBASE>10</TICKSPERBASE>
