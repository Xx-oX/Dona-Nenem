        <SHORT-NAME>TaskConfigures</SHORT-NAME>
        <ELEMENTS>
            <TASK>
                <SHORT-NAME>TaskA</SHORT-NAME>
                <PRIORITY>3</PRIORITY>
                <ACTIVATION>10</ACTIVATION>
                <STACK>512</STACK>
                <TYPE>EXTENDED</TYPE>
                <SCHEDULE>FULL</SCHEDULE>
                <EVENT>
                  <SHORT-NAME>EventP</SHORT-NAME>
                </EVENT>
                <AUTOSTART>
                    <VALUE>FALSE</VALUE>
                </AUTOSTART>
            </TASK>
            <TASK>
                <SHORT-NAME>TaskB</SHORT-NAME>
                <PRIORITY>3</PRIORITY>
                <ACTIVATION>10</ACTIVATION>
