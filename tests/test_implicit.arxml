        <SHORT-NAME>AlarmConfigures</SHORT-NAME>
        <ELEMENTS>
            <ALARM>
                <SHORT-NAME>epAlarmA</SHORT-NAME>
                <COUNTER>
                    <SHORT-NAME>CounterA</SHORT-NAME>
                </COUNTER>
