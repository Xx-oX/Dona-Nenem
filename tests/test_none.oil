    };

    SCHEDULETABLE ScheduleTableB{
        DURATION = 40;
        INITOFFSET = 13;
        FINALDELAY = 27;
        SYNCSTRATEGY = NONE;
        REPEATING = TRUE;
        COUNTER = CounterB;
        CALLBACKALARM = epAlarmB;
        EXPIRYPOINT EP1 {
            OFFSET = 13;
            MAXSHORTEN = 0;
            MAXLENGTHEN = 0;
            TASKACTIVATION = TaskA, TaskB;
        };
        AUTOSTART =FALSE;
    };

};
